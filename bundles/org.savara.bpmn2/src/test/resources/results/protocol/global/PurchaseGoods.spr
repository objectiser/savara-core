import xsd "{http://www.jboss.org/examples/store}BuyRequest" as BuyRequest from "platform:/resource/purchasing/store.xsd";
import xsd "{http://www.jboss.org/examples/creditAgency}CreditCheckRequest" as CreditCheck from "platform:/resource/purchasing/creditAgency.xsd";
import xsd "{http://www.jboss.org/examples/creditAgency}CustomerUnknown" as CustomerUnknown from "platform:/resource/purchasing/creditAgency.xsd";
import xsd "{http://www.jboss.org/examples/creditAgency}CreditRating" as CreditRating from "platform:/resource/purchasing/creditAgency.xsd";
import xsd "{http://www.jboss.org/examples/store}AccountNotFound" as AccountNotFound from "platform:/resource/purchasing/store.xsd";
import xsd "{http://www.jboss.org/examples/store}BuyFailed" as BuyFailed from "platform:/resource/purchasing/store.xsd";
import xsd "{http://www.jboss.org/examples/logistics}DeliveryRequest" as DeliveryRequest from "platform:/resource/purchasing/logistics.xsd";
import xsd "{http://www.jboss.org/examples/logistics}DeliveryConfirmed" as DeliveryConfirmed from "platform:/resource/purchasing/logistics.xsd";
import xsd "{http://www.jboss.org/examples/store}BuyConfirmed" as BuyConfirmed from "platform:/resource/purchasing/store.xsd";
[[ Type(prefix=ns2,namespace=http://www.jboss.org/examples/logistics) ]]
[[ Type(prefix=ns1,namespace=http://www.jboss.org/examples/store) ]]
[[ Type(prefix=ns3,namespace=http://www.jboss.org/examples/creditAgency) ]]
[[ SourceComponent(id=Choreography_1) ]]
[[ Namespace(name=http://www.savara.org/examples,role=Buyer) ]]
[[ Namespace(name=http://www.savara.org/examples,role=CreditAgency) ]]
[[ Namespace(name=http://www.savara.org/examples,role=Store) ]]
[[ Namespace(name=http://www.savara.org/examples,role=Logistics) ]]
[[ Type(prefix=tns,namespace=http://www.savara.org/examples) ]]
protocol PurchaseGoods(role Buyer) {
	Store introduces CreditAgency;
	Buyer introduces Store;
	[[ Correlation(request=buy) ]]
	buy(BuyRequest) from Buyer to Store;
	[[ Correlation(request=creditCheck) ]]
	creditCheck(CreditCheck) from Store to CreditAgency;
	choice at CreditAgency {
		[[ Fault(name=CustomerUnknown) ]]
		[[ Correlation(replyTo=creditCheck) ]]
		creditCheck(CustomerUnknown) from CreditAgency to Store;
		[[ Fault(name=AccountNotFound) ]]
		[[ Correlation(replyTo=buy) ]]
		buy(AccountNotFound) from Store to Buyer;
	} or {
		[[ Correlation(replyTo=creditCheck) ]]
		creditCheck(CreditRating) from CreditAgency to Store;
		choice at Store {
			[[ Fault(name=InsufficientCredit) ]]
			[[ Correlation(replyTo=buy) ]]
			buy(BuyFailed) from Store to Buyer;
		} or {
			Store introduces Logistics;
			[[ Correlation(request=delivery) ]]
			delivery(DeliveryRequest) from Store to Logistics;
			[[ Correlation(replyTo=delivery) ]]
			delivery(DeliveryConfirmed) from Logistics to Store;
			[[ Correlation(replyTo=buy) ]]
			buy(BuyConfirmed) from Store to Buyer;
		}
	}
}
