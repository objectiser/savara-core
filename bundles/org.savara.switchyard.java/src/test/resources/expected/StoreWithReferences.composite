<composite xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912"
		xmlns:tuscany="http://tuscany.apache.org/xmlns/sca/1.1"
		targetNamespace="http://www.savara.org/examples/Store"
		name="Store" >
	<component name="StoreComponent">
		<implementation.java class="org.savara.examples.store.StoreImpl" />
		<service name="Store">
			<interface.java interface="org.savara.examples.store.Store" />
			<binding.ws uri="http://localhost:8080/StoreComponent" />
		</service>
		<reference name="creditAgency">
			<binding.ws uri="http://localhost:8080/CreditAgencyComponent" />
		</reference>
		<reference name="logistics">
			<binding.ws uri="http://localhost:8080/LogisticsComponent" />
		</reference>
	</component>
</composite>
