<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="Logistics" targetNamespace="http://www.savara.org" xmlns:ns2="http://www.jboss.org/examples/logistics" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://www.savara.org" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
  <wsdl:message name="deliverResponse">
    <wsdl:part name="content" type="ns2:DeliveryConfirmed">
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="deliverRequest">
    <wsdl:part name="content" type="ns2:DeliveryRequest">
    </wsdl:part>
  </wsdl:message>
  <wsdl:portType name="Logistics">
    <wsdl:operation name="deliver">
      <wsdl:input message="tns:deliverRequest">
    </wsdl:input>
      <wsdl:output message="tns:deliverResponse">
    </wsdl:output>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="LogisticsBinding" type="tns:Logistics">
    <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="deliver">
      <soap:operation soapAction="" style="rpc"/>
      <wsdl:input>
        <soap:body use="literal" namespace="http://www.savara.org"/>
      </wsdl:input>
      <wsdl:output>
        <soap:body use="literal" namespace="http://www.savara.org"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="LogisticsService">
    <wsdl:port name="LogisticsPort" binding="tns:LogisticsBinding">
      <soap:address location="http://localhost:8080/LogisticsService/LogisticsPort"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>
